{
  "name": "server",
  "version": "1.0.0",
  "description": "server for app",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf dist",
    "babel": "babel ./src/main.js --out-dir dist",
    "build": "npm-run-all clear babel",
    "start": "node ./dist/main.js",
    "prod": "cross-env NODE_ENV=production npm-run-all build start",
    "dev": "cross-env NODE_ENV=development nodemon src/main.js --exec babel-node --presets @babel/preset-env"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
